<Application x:Class="UPSBatteryController.Presentation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UPSBatteryController.Presentation"
             ShutdownMode="OnExplicitShutdown">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionaries/ImagesDictionary.xaml"/>
                <ResourceDictionary Source="Dictionaries/BrushesDictionary.xaml"/>
                <ResourceDictionary Source="Dictionaries/StylesDictionary.xaml"/>
                <ResourceDictionary Source="Dictionaries/ConvertersDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
